package week1.day1;



public class mobile {
public void makeCall() {
	System.out.println("calling");
}

public void messageSend() {
	System.out.println("message sent");
      }
	public static void main(String[] args) {

		mobile mymobile = new mobile() ;
mymobile.makeCall();

 mymobile.messageSend(); }
	
	
	
	
	
}
	

